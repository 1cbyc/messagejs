{
  "name": "@messagejs/client",
  "version": "0.1.0",
  "description": "MessageJS Client SDK - Lightweight SDK for sending messages from web apps",
  "keywords": [
    "messaging",
    "whatsapp",
    "telegram",
    "sms",
    "sdk",
    "chat",
    "messagejs"
  ],
  "author": "Isaac Emmanuel",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/1cbyc/messagejs.git",
    "directory": "packages/client"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "README.md"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "rollup -c rollup.config.mjs",
    "dev": "rollup -c rollup.config.mjs --watch",
    "clean": "rm -rf dist",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "npm run build"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {},
  "peerDependencies": {
    "@messagejs/shared-types": "*"
  },
  "devDependencies": {
    "@rollup/plugin-typescript": "^11.1.6",
    "@types/node": "^20.10.0",
    "rollup": "^4.9.0",
    "typescript": "^5.3.3"
  }
}
